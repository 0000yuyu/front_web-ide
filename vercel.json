{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://43.202.161.69:8080/$1"
    },
    {
      "source": "/api/ws(.*)",
      "destination": "ws://43.202.161.69:8080/ws/chat$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
