{
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/api/(.*)",
      "dest": "http://43.202.161.69:8080/"
    },
    {
      "src": "^/api/ws(.*)",
      "dest": "ws://43.202.161.69:8080/ws/chat"
    }
  ]
}
